{
  "AHK Boilerplate": {
    "prefix": "q",
    "body": [
      "; ==================================================================================",
      "; Name ..........: Name.ahk",
      "; Description ...: Description",
      "; Shortcut ......: Shortcut Keys",
      "; Modified ......: DD-MM-YY",
      "; Author ........: Samyak Bumb",
      "; ==================================================================================\n",
      "#NoEnv",
      "#MaxMem 4095",
      "#SingleInstance, Force",
      "SendMode Input",
      "SetWorkingDir %A_ScriptDir%\n",
      "if not A_IsAdmin\n{\n\tRun *RunAs \"%A_ScriptFullPath%\"\n\tExitApp\n}\n; *****************************************************************************",
      "${1:Start Script}\n; *****************************************************************************\n"
    ]
  },
  "Line Comment": {
    "prefix": "comment",
    "body": [
      "; *****************************************************************************",
      "; \t${1:Comment}",
      "; *****************************************************************************"
    ]
  }
}
