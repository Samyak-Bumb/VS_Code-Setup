{
  "AHK Boilerplate": {
    "prefix": "q",
    "body": [
      "; ==================================================================================",
      "; Name ..........: ${1:Name}.ahk",
      "; Description ...: Description",
      "; Modified ......: ${2:DD-MM-YY}",
      "; Author ........: Samyak Bumb",
      "; ==================================================================================\n",
      "#Warn, UseUnsetLocal",
      "#NoEnv",
      "#MaxMem 4095",
      "#SingleInstance Force",
      "SendMode Input",
      "SetWorkingDir %A_ScriptDir%\n",
      "if not A_IsAdmin\n{\n\tRun *RunAs \"%A_ScriptFullPath%\"\n\tExitApp\n}\n\nMenu, Tray, Icon, F:\\ahk\\ico\\$3\n\n; *****************************************************************************",
      "$0\n; *****************************************************************************\n"
    ]
  },
  "Line Comment": {
    "prefix": "comment",
    "body": [
      "; *****************************************************************************",
      "; ${1:Comment}",
      "; *****************************************************************************"
    ]
  },
  "Square Comment": {
    "prefix": "commentsq",
    "body": [
      ";[===================]",
      ";[                   ]",
      ";[\t${1:Comment Here}     ]",
      ";[                   ]",
      ";[===================]"
    ]
  },
}
