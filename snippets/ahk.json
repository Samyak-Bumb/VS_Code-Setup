{
  "AHK Boilerplate": {
    "prefix": "q",
    "body": [
      "; ==================================================================================",
      "; Name ..........: Name.ahk",
      "; Description ...: Description",
      "; Shortcut ......: Shortcut Keys",
      "; Modified ......: DD-MM-YY",
      "; Author ........: Samyak Bumb",
      "; ==================================================================================\n",
      "#NoEnv",
      "#MaxMem 4095",
      "#SingleInstance, Force\n",
      "SendMode Input",
      "SetWorkingDir %A_ScriptDir%\n",
      "if !A_IsAdmin",
      "\tRun *RunAs \"%A_ScriptFullPath%\"\n; *****************************************************************************\n",
      "${1:Start Script}\n; *****************************************************************************\n",
      "#+R::",
      "\tReload\n",
    ]
  },
  "Line Comment": {
    "prefix": "comment",
    "body": [
      "; *****************************************************************************",
      "; \t${1:Comment}",
      "; *****************************************************************************"
    ]
  }
}
