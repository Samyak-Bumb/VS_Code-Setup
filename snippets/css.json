// Created/Written By Samyak Bumb
{
	"CSS Boilerplate with Loader By Samyak": {
		"prefix": "q",
		"body": [
			"@import url('https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap');\n",
			"*{",
			"\tmargin: 0;",
			"\tpadding: 0;",
			"\tbox-sizing: border-box;",
			"\tfont-family: 'Poppins', sans-serif\n}",
			"::selection{",
			"\ttext-shadow: none;",
			"\tbackground: #ea4c89\n}", // "a8cefc", "fad00052"
			"body{",
			"\tmin-height: 100vh;",
			"\tcolor: #eee;",
			"\tbackground: #000",
			"}\n${0:}",
		]
	},
	"RGBA": {
		"prefix": "ra",
		"body": "rgba(${1:0},${2:0},${3:0},${4:0.1})"
	},
	"Display: Flex + Center": {
		"prefix": "df",
		"body": "display: flex;\njustify-content: center;\nalign-items: ${0:center};"
	},
	"Display: Grid + Center": {
		"prefix": "dg",
		"body": "display: grid;\nplace-items: center;"
	},
	"Box Shadow": {
		"prefix": "bxs",
		"body": "box-shadow: ${1:0} ${2:10}px ${3:20}px rgba(${4:0},${5:0},${6:0},${7:0.1});"
	},
	"Media Query": {
		"prefix": "@m",
		"body": "@media screen and (max-width: 769px){\n\t${1:}{\n\t\t${0:}\n\t}\n}"
	},
	"KeyFrames": {
		"prefix": "@k",
		"body": "@keyframes ${1:ani}{\n\t0%{\n\t\t${0:}\n\t}\n}"
	},
	"Font Face": {
		"prefix": "@f",
		"body": "@font-face {\n\tfont-family: samyak;\n\tsrc: url(\"./font/${0:.ttf}\");\n}"
	},
	"Top & Left: 0": {
		"prefix": "tl",
		"body": "top: 0;\nleft: 0;"
	},
	"Width & Height: 100%": {
		"prefix": "wh",
		"body": "width: ${1:100%};\nheight: ${0:100%};"
	},
	"Box Model": {
		"prefix": "bxm",
		"body": [
			"display: ${1:block};",
			"position: ${2:absolute};",
			"width: ${3: 50px};",
			"height: ${4: 50px};",
			"background : #${0:000}"
		]
	},
	"Border": {
		"prefix": "bd",
		"body": "border: ${1:1}px solid ${0:#000};"
	},
	"Align items: center": {
		"prefix": "ai",
		"body": "align-items: center;"
	},
	"Border & Outline: none": {
		"prefix": "bo",
		"body": "border: none;\noutline: none;"
	},
	"Hover Psedu": {
		"prefix": ":h",
		"body": ":hover{\n\t${0:}\n}"
	},
	"Root Psedu": {
		"prefix": ":r",
		"body": ":root{\n\t${0:}\n}"
	},
	"Before Psedu": {
		"prefix": ":b",
		"body": "::before{\n\tcontent: '';\n\t${0:}\n}"
	},
	"After Psedu": {
		"prefix": ":a",
		"body": "::after{\n\tcontent: '';\n\t${0:}\n}"
	},
	"Last Psedu": {
		"prefix": ":l",
		"body": ":last-child{\n\t${0:}\n}"
	},
	"nth Child": {
		"prefix": ":n",
		"body": ":nth-child(${1:}){\n\t${0:}\n}"
	},
	"Placeholder Psedu": {
		"prefix": ":p",
		"body": "::placeholder(${1:}){\n\t${0:}\n}"
	},
	"Active Psedu": {
		"prefix": ":ac",
		"body": ":active{\n\t${0:}\n}"
	},
	"Background with Linear-Gradient": {
		"prefix": "bgl",
		"body": "background: linear-gradient(${1:90}deg,#${2:696969},#${3:696969});"
	},
	"Color": {
		"prefix": "c",
		"body": "color: #${0:000};"
	},
	"Background": {
		"prefix": "bg",
		"body": "background: #${0:000};"
	},
	"Pointer-Events: none": {
		"prefix": "pen",
		"body": "pointer-events: none;"
	},
	"Filter with Blur": {
		"prefix": "fr",
		"body": "filter: blur(${0:1}px);"
	},
	"Transition": {
		"prefix": "tra",
		"body": "transition: all ${1:300}ms ${0:};"
	},
	"Minimum Height": {
		"prefix": "mh",
		"body": "min-height: ${0:100vh};"
	},
	"Comment Block": {
		"prefix": "comment-block",
		"body": "\n/**\n *\n * ${1:Block comment}\n *\n */\n\n"
	},
	"Comment Section Header": {
		"prefix": "comment-section-header",
		"body": "\n/*=============================================\n=            ${1:Section comment block}            =\n=============================================*/\n\n$2\n\n"
	},
	"Comment Section Footer": {
		"prefix": "comment-section-footer",
		"body": "\n/*=====  End of ${1:Section comment block}  ======*/\n\n"
	},
	"Comment Section": {
		"prefix": "comment-section",
		"body": "\n/*=============================================\n=            ${1:Section comment block}            =\n=============================================*/\n\n$2\n\n/*=====  End of ${1:Section comment block}  ======*/\n\n"
	},
	"Comment Subsection": {
		"prefix": "comment-subsection",
		"body": "\n/*----------  ${1:Subsection comment block}  ----------*/\n\n"
	},
	"Comment": {
		"prefix": "comment",
		"body": "\n/* ${1:Comment} */\n\n"
	},
	"Claymorphism": {
		"prefix": "clay",
		"body": [
			"backdrop-filter: blur(5px);",
			"border-radius: 26px;",
			"box-shadow: 35px 35px 68px 0 rgba(145,192,255,0.5), inset -8px -8px 16px 0 rgba(145,192, 55,0.6), inset 0 11px 28px 0 #fff;",
			"background: #fff;"
		]
	}
}
