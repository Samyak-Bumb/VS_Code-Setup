// Created/Written By Samyak Bumb
{
	"CSS Boilerplate with Loader By Samyak": {
		"prefix": "q",
		"body": [
			"@import url('https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap');",
			"",
			"*{",
			"\tmargin: 0;",
			"\tpadding: 0;",
			"\tbox-sizing: border-box;",
			"\tfont-family: 'Poppins', sans-serif;",
			"}",
			"::selection{",
			"\ttext-shadow: none;",
			"\tbackground: #ea4c89;",// "a8cefc", "fad00052"
			"}",
			"body{",
			"\tmin-height: 100vh;",
			"\tcolor: #eee;",
			"\tbackground: #000",
			"}",
			"#loader{",
			"\tposition: fixed;",
			"\twidth: 100%;",
			"\theight: 100%;",
			"\topacity: 1;",
			"\tz-index: 9999;",
			"\ttransform: translateY(0);",
			"\tanimation: fadeinall 1s normal both;",
			"\tanimation-delay: 300ms;",
			"\tanimation-fill-mode: forwards",
			"}",
			"@keyframes fadeinall{",
			"\t0%{opacity: 1}",
			"\t97%{opacity: 0}",
			"\t98%{opacity: 0; transform: translateY(0)}",
			"\t99%{opacity: 0; transform: translateY(-100%)}",
			"\t100%{opacity: 0; z-index: -1}",
			"}",
			"${0:}"
		]
	},
	"RGBA": {
		"prefix": "ra",
		"body": "rgba(${1:0},${2:0},${3:0},${4:0.1})"
	},
	"Display: Flex + Center": {
		"prefix": "df",
		"body": [
			"display: flex;",
			"justify-content: center;",
			"align-items: ${0:center};"
		]
	},
	"Display: Grid + Center": {
		"prefix": "dg",
		"body": [
			"display: grid;",
			"place-items: center;"
		]
	},
	"Box Shadow": {
		"prefix": "bxs",
		"body": "box-shadow: ${1:0} ${2:10}px ${3:20}px rgba(${4:0},${5:0},${6:0},${7:0.1});"
	},
	"Media Query": {
		"prefix": "@q",
		"body": [
			"@media screen and (max-width: 769px){",
			"\t${1:}{",
			"\t\t${0:}",
			"\t}",
			"}"
		]
	},
	"KeyFrames": {
		"prefix": "@k",
		"body": [
			"@keyframes ${1:ani}{",
			"\t0%{",
			"\t\t${0:}",
			"\t}",
			"}"
		]
	},
	"Font Face": {
		"prefix": "@f",
		"body": [
			"@font-face {",
			"\tfont-family: Samyak;",
			"\tsrc: url(\"./font/${0:.ttf}\");",
			"}"
		]
	},
	"Top & Left: 0": {
		"prefix": "tl",
		"body": [
			"top: 0;",
			"left: 0;"
		]
	},
	"Width & Height: 100%": {
		"prefix": "wh",
		"body": [
			"width: ${1:100%};",
			"height: ${0:100%};"
		]
	},
	"Box Model":{
		"prefix":"bxm",
		"body":[
			"display: ${1:block} ;",
			"position: ${2:absolute};",
			"width: ${3: 50px};",
			"height: ${4: 50px};",
			"background : #${0:000}"
		]
	},
	"Border": {
		"prefix": "bd",
		"body": "border: ${1:1}px solid ${0:#000};"
	},
	"Align items: center": {
		"prefix": "ai",
		"body": "align-items: center"
	},
	"Border & Outline: none": {
		"prefix": "bo",
		"body": [
			"border: none;",
			"outline: none;"
		]
	},
	"Hover Psedu": {
		"prefix": ":h",
		"body": [
			":hover{",
			"\t${0:}",
			"}"
		]
	},
	"Root Psedu": {
		"prefix": ":r",
		"body": [
			":root{",
			"\t${0:}",
			"}"
		]
	},
	"Before Psedu": {
		"prefix": ":b",
		"body": [
			"::before{",
			"\tcontent: '';",
			"\t${0:}",
			"}"
		]
	},
	"After Psedu": {
		"prefix": ":a",
		"body": [
			"::after{",
			"\tcontent: '';",
			"\t${0:}",
			"}"
		]
	},
	"Last Psedu": {
		"prefix": ":l",
		"body": [
			":last-child{",
			"\t${0:}",
			"}"
		]
	},
	"nth Child": {
		"prefix": ":n",
		"body": [
			":nth-child(${1:}){",
			"\t${0:}",
			"}"
		]
	},
	"Placeholder Psedu": {
		"prefix": ":p",
		"body": [
			"::placeholder{",
			"\t${0:}",
			"}"
		]
	},
	"Active Psedu": {
		"prefix": ":ac",
		"body": [
			":active{",
			"\t${0:}",
			"}"
		]
	},
	"Background with Linear-Gradient": {
		"prefix": "bgl",
		"body": "background: linear-gradient(${1:69}deg,#${2:696969},#${3:696969});"
	},
	"Color": {
		"prefix": "c",
		"body": "color: #${0:000};"
	},
	"Background": {
		"prefix": "bg",
		"body": "background: #${0:000};"
	},
	"Pointer-Events: none": {
		"prefix": "pen",
		"body": "pointer-events: none;"
	},
	"Filter with Blur": {
		"prefix": "fr",
		"body": "filter: blur(${0:1}px);"
	},
	"Transition": {
		"prefix": "tra",
		"body": "transition: all ${1:300}ms ${0:};"
	},
	"Minimum Height": {
		"prefix": "mh",
		"body": "min-height: ${0:100vh};"
	}
}
