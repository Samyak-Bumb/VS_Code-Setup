// Created/Written By Samyak Bumb
{
  "CSS Boilerplate with Loader By Samyak": {
    "prefix": "q",
    "body": [
      "@import url(\"https://fonts.googleapis.com/css2?family=Poppins:wght@200;300;400;500;600;700&display=swap\");\n",
      "*{",
      "\tmargin: 0;",
      "\tpadding: 0;",
      "\tbox-sizing: border-box;",
      "\tfont-family: \"Poppins\", sans-serif\n}",
      "::selection{",
      "\ttext-shadow: none;",
      "\tbackground: #ea4c89\n}", // "a8cefc", "fad00052"
      "body{",
      "\tmin-height: 100vh;",
      "\tcolor: #eee;",
      "\tbackground: #000",
      "}\n\n$0"
    ]
  },
  "RGBA": {
    "prefix": "ra",
    "body": "rgba(${1:0},${2:0},${3:0},.${4:1})"
  },
  "Display: Flex + Center": {
    "prefix": "df",
    "body": "display: flex;\njustify-content: ${1:center};\nalign-items: ${0:center};"
  },
  "Display: Grid + Center": {
    "prefix": "dg",
    "body": "display: grid;\nplace-items: center;"
  },
  "Box Shadow": {
    "prefix": "bxs",
    "body": "box-shadow: ${1:0} ${2:10}px ${3:20}px rgba(${4:0},${5:0},${6:0},.${7:1});"
  },
  "Media Query": {
    "prefix": "@m",
    "body": "@media screen and (max-width: 769px){\n\t${1:}{\n\t\t${0:}\n\t}\n}"
  },
  "KeyFrames": {
    "prefix": "@k",
    "body": "@keyframes ${1:ani}{\n\t0%{\n\t\t${0:}\n\t}\n}"
  },
  "Font Face": {
    "prefix": "@f",
    "body": "@font-face {\n\tfont-family: samyak;\n\tsrc: url(\"./font/${0:.ttf}\");\n}"
  },
  "Top & Left: 0": {
    "prefix": "tl",
    "body": "top: 0;\nleft: 0;"
  },
  "Width & Height: 100%": {
    "prefix": "wh",
    "body": "width: ${1:100%};\nheight: ${0:100%};"
  },
  "Border": {
    "prefix": "bd",
    "body": "border: ${1:1}px solid ${0:#000};"
  },
  "Align items: center": {
    "prefix": "ai",
    "body": "align-items: center;"
  },
  "Border & Outline: none": {
    "prefix": "bo",
    "body": "border: none;\noutline: none;"
  },
  "Hover Psedu": {
    "prefix": ":h",
    "body": ":hover{\n\t${0:}\n}"
  },
  "Root Psedu": {
    "prefix": ":r",
    "body": ":root{\n\t${0:}\n}"
  },
  "Before Psedu": {
    "prefix": ":b",
    "body": "::before{\n\tcontent: '';\n\t${0:}\n}"
  },
  "After Psedu": {
    "prefix": ":a",
    "body": "::after{\n\tcontent: '';\n\t${0:}\n}"
  },
  "Last Psedu": {
    "prefix": ":l",
    "body": ":last-child{\n\t${0:}\n}"
  },
  "nth Child": {
    "prefix": ":n",
    "body": ":nth-child(${1:}){\n\t${0:}\n}"
  },
  "Placeholder Psedu": {
    "prefix": ":p",
    "body": "::placeholder(${1:}){\n\t${0:}\n}"
  },
  "Active Psedu": {
    "prefix": ":ac",
    "body": ":active{\n\t${0:}\n}"
  },
  "Background with Linear-Gradient": {
    "prefix": "bgl",
    "body": "background: linear-gradient(${1:90}deg,#${2:696969},#${3:696969});"
  },
  "Color": {
    "prefix": "c",
    "body": "color: #${0:000};"
  },
  "Background": {
    "prefix": "bg",
    "body": "background: #${0:000};"
  },
  "Pointer-Events: none": {
    "prefix": "pen",
    "body": "pointer-events: none;"
  },
  "Filter with Blur": {
    "prefix": "fr",
    "body": "filter: blur(${0:1}px);"
  },
  "Root Variable": {
    "prefix": "var_root",
    "body": "--${1:variableName}: $0;"
  },
  "Variable": {
    "prefix": "var",
    "body": "var(--$0)"
  },
  "Transition": {
    "prefix": "tra",
    "body": "transition: all ${1:300}ms;"
  },
  "Minimum Height": {
    "prefix": "mh",
    "body": "min-height: ${0:100vh};"
  },
  "CSS Reset": {
    "prefix": "css_Reset",
    "body": [
      "html, body, p, ol, ul, li, dl, dt, dd, blockquote, figure, fieldset, legend, textarea, pre, iframe, hr, h1, h2, h3, h4, h5, h6{\n\tmargin: 0;\n\tpadding: 0\n}",
      "h1, h2, h3, h4, h5, h6{\n\tfont-size: 100%;\n\tfont-weight: normal\n}",
      "ul{\n\tlist-style: none\n}",
      "button, input, select{\n\tmargin: 0\n}",
      "html{\n\tbox-sizing: border-box;\n}",
      "*, *::before, *::after{\n\tbox-sizing: inherit;\n}",
      "img, video{\n\theight: auto;\n\tmax-width: 100%;\n}",
      "iframe{\n\tborder: 0\n}",
      "table{\n\tborder-collapse: collapse;\n\tborder-spacing: 0\n}",
      "td, th{\n\tpadding: 0;\n}\n"
    ]
  },
  "CSS Reset Minify": {
    "prefix": "css_Reset_min",
    "body": "html,body,p,ol,ul,li,dl,dt,dd,blockquote,figure,fieldset,legend,textarea,pre,iframe,hr,h1,h2,h3,h4,h5,h6{margin:0;padding:0}h1,h2,h3,h4,h5,h6{font-size:100%;font-weight:normal}ul{list-style:none}button,input,select{margin:0}html{box-sizing:border-box}*,*::before,*::after{box-sizing:inherit}img,video{height:auto;max-width:100%}iframe{border:0}table{border-collapse:collapse;border-spacing:0}td,th{padding:0}"
  },
  "Comment": {
    "prefix": "comment",
    "body": "\n/* ${0:Comment} */\n"
  },
  "Comment Block": {
    "prefix": "comment-block",
    "body": "\n\n/**\n *\n * ${0:Block comment}\n *\n **/\n"
  },
  "Comment Section": {
    "prefix": "comment-section",
    "body": "\n/*=============================================\n=         Start of ${1:Section comment block}      =\n=============================================*/\n\n${0:}\n\n/*======  End of ${1:Section comment block}  ======*/\n"
  },
  "Comment Section End": {
    "prefix": "comment-section-header",
    "body": "\n/*=============================================\n=         Start of ${0:Section comment block}      =\n=============================================*/\n"
  },
  "Comment Section Begin": {
    "prefix": "comment-section-footer",
    "body": "\n/*=====  End of ${1:Section comment block}  ======*/\n"
  },
  "Comment Line Dash": {
    "prefix": "comment-line-dash",
    "body": "\n/*----------  ${0:Line comment block}  ----------*/\n"
  },
  "Comment Line Star": {
    "prefix": "comment-line-star",
    "body": "\n/***********  ${0:Star comment line}  ***********/\n"
  },
  "Comment Line Equal": {
    "prefix": "comment-line-equal",
    "body": "\n/*==========  ${0:Equal comment line}  ==========*/\n"
  },
  "Comment TO-DO": {
    "prefix": "comment-todo",
    "body": "/**\n\n\tTODO:\n\t\t- ${1:First todo}\n\t\t- ${0:Second todo}\n\n */\n\n"
  },
  "Small Devices and Larger": {
    "prefix": "small-devices-and-up-media-query",
    "body": "@media (min-width: 576px){\n$0\t${1:Rules}\n"
  },
  "Medium Devices and Larger": {
    "prefix": "medium-devices-and-up-media-query",
    "body": "@media (min-width: 768px){\n$0\t${1:Rules}\n}"
  },
  "Large Devices and Larger": {
    "prefix": "large-devices-and-up-media-query",
    "body": "@media (min-width: 992px){\n$0\t${1:Rules}\n}"
  },
  "Small devices devices and Smaller": {
    "prefix": "small-devices-and-down-media-query",
    "body": "@media (max-width: 767.98px){\n$0\t${1:Rules}\n}"
  },
  "Medium devices devices and Smaller": {
    "prefix": "medium-devices-and-down-media-query",
    "body": "@media (max-width: 991.98px){\n$0\t${1:Rules}\n}"
  },
  "Large devices devices and Smaller": {
    "prefix": "large-devices-and-down-media-query",
    "body": "@media (max-width: 1199.98px){\n$0\t${1:Rules}\n}"
  },
  "Extra Small devices only": {
    "prefix": "extra-small-devices-only-media-query",
    "body": "@media (max-width: 575.98px){\n$0\t${1:Rules}\n}"
  },
  "Small devices only": {
    "prefix": "small-devices-only-media-query",
    "body": "@media (min-width: 576px) and (max-width: 767.98px){\n$0\t${1:Rules}\n}"
  },
  "Medium devices only": {
    "prefix": "medium-devices-only-media-query",
    "body": "@media (min-width: 768px) and (max-width: 991.98px){\n$0\t${1:Rules}\n}"
  },
  "Large devices only": {
    "prefix": "large-devices-only-media-query",
    "body": "@media (min-width: 992px) and (max-width: 1199.98px){\n$0\t${1:Rules}\n}"
  },
  "Extra Large Devices Only": {
    "prefix": "extra-large-devices-only-media-query",
    "body": "@media (min-width: 1200px){\n$0\t${1:Rules}\n}"
  }
}
