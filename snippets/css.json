// Created/Written By Samyak Bumb
{
  "CSS Boilerplate with Loader By Samyak": {
    "prefix": "q",
    "body": [
      "@import url(\"https://fonts.googleapis.com/css2?family=Poppins:wght@200;400;500;700&display=swap\");\n",
      "*{",
      "\tmargin: 0;",
      "\tpadding: 0;",
      "\tbox-sizing: border-box;",
      "\tfont-family: \"Poppins\", sans-serif\n}\n:root{\n\t--light: #fbfbfb;\n\t--dark: #121212;\n\t--selection: #ea4c89;\n}",
      "::selection{",
      "\tcolor: var(--light);",
      "\tbackground: var(--selection)\n}",
      /*
      Light Yellow (White Bg *REQUIRED*): fad00052
      Light Green (White Bg *REQUIRED*): a7f5a9
      Light Crimson: ea4c89
      Light Blue: a8cefc
      Light Green: 00d1b2
      Light Cyan: 9bf0f0
      Light Purple: 5e72e4
      Light Pink: f775C1
      5773ff
      */
      "body{",
      "\tmin-height: 100vh;",
      "\tcolor: var(--light);",
      "\tbackground: var(--dark)",
      "}\n\n$0"
    ]
  },
  "RGBA": {
    "prefix": "ra",
    "body": "rgba(${1:0},${2:0},${3:0},.${4:1})"
  },
  "Display: Flex + Center": {
    "prefix": "df",
    "body": "display: flex;\njustify-content: ${1:center};\nalign-items: ${0:center};"
  },
  "Display: Grid + Center": {
    "prefix": "dg",
    "body": "display: grid;\nplace-items: center;"
  },
  "Box Shadow": {
    "prefix": "bxs",
    "body": "box-shadow: ${1:0} ${2:0} ${3:0} rgba(${4:0},${5:0},${6:0},.${7:1});"
  },
  "Scrollbar": {
    "prefix": "scrollbar",
    "body": [
      "::-webkit-scrollbar{",
      "\twidth: 10px;",
      "\tbackground: #c4c8d4",
      "}",
      "::-webkit-scrollbar-thumb{",
      "\tborder-radius: 10px;",
      "\tbackground: #9ca8b4;",
      "\tborder: 2px solid #2b333b;",
      "}",
      "::-webkit-scrollbar-track{",
      "\tborder-radius: 6px;",
      "\tbackground: #0000;",
      "}"
    ]
  },
  "Media Query": {
    "prefix": "@m",
    "body": "@media screen and (max-width: 769px){\n\t${1:}{\n\t\t${0:}\n\t}\n}"
  },
  "KeyFrames": {
    "prefix": "@k",
    "body": "@keyframes ${1:ani}{\n\t0%{\n\t\t${0:}\n\t}\n}"
  },
  "Font Face": {
    "prefix": "@f",
    "body": "@font-face {\n\tfont-family: samyak;\n\tsrc: url(\"./font/${0:.ttf}\");\n}"
  },
  "Top & Left: 0": {
    "prefix": "tl",
    "body": "top: 0;\nleft: 0;"
  },
  "Width & Height: 100%": {
    "prefix": "wh",
    "body": "width: ${1:100%};\nheight: ${0:100%};"
  },
  "Border": {
    "prefix": "bd",
    "body": "border: ${1:1}px solid ${0:#000};"
  },
  "Align items: center": {
    "prefix": "ai",
    "body": "align-items: center;"
  },
  "Border & Outline: none": {
    "prefix": "bo",
    "body": "border: none;\noutline: none;"
  },
  "Hover": {
    "prefix": ":h",
    "body": ":hover{\n\t${0:}\n}"
  },
  "Root": {
    "prefix": ":r",
    "body": ":root{\n\t${0:}\n}"
  },
  "Before": {
    "prefix": ":b",
    "body": "::before{\n\tcontent: '';\n\t${0:}\n}"
  },
  "After": {
    "prefix": ":a",
    "body": "::after{\n\tcontent: '';\n\t${0:}\n}"
  },
  "Last Child": {
    "prefix": ":l",
    "body": ":last-child{\n\t${0:}\n}"
  },
  "First Child": {
    "prefix": ":fc",
    "body": ":first-child{\n\t${0:}\n}"
  },
  "nth Child": {
    "prefix": ":n",
    "body": ":nth-child(${1:}){\n\t${0:}\n}"
  },
  "nth Child Even": {
    "prefix": ":n-e",
    "body": ":nth-child(${1:2n}){\n\t${0:}\n}"
  },
  "nth Child Odd": {
    "prefix": ":n-o",
    "body": ":nth-child(${1:odd}){\n\t${0:}\n}"
  },
  "Placeholder": {
    "prefix": ":p",
