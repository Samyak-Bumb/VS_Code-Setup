// Created/Written By Samyak Bumb
{
	"C Boilerplate": {
		"prefix": "q",
		"body": [
			"#include <stdio.h>\n",
			"int main()\n{\n\t$0\n",
			"\treturn 0;\n}"
		],
	},
	"Print": {
		"prefix": "pr",
		"body": "printf($0);"
	},
	"Scan": {
		"prefix": "sca",
		"body": "scanf(\"$1\", &$2);"
	},
	"Intger": {
		"prefix": "in",
		"body": "int ${1:name} = $2;"
	},
	"Float": {
		"prefix": "flo",
		"body": "float ${1:name} = $2;"
	},
	"Double": {
		"prefix": "dou",
		"body": "double ${1:name} = $2;"
	},
	"Character": {
		"prefix": "cha",
		"body": "char ${1:name} = $2;"
	},
	"for": {
		"prefix": "for",
		"body": "for (${size_t} ${i} = ${1:0}; ${i} < ${2:length}; ${i}++)\n{\n\t$3\n}"
	},
	"forr": {
		"prefix": "forr",
		"body": "for (int ${i} = ${1:length} - 1; ${i} >= ${2:0}; ${i}--)\n{\n\t$3\n}"
	},
	"while": {
		"prefix": "while",
		"body": "while ($1)\n{\n\t$2\n}"
	},
	"if": {
		"prefix": "if",
		"body": "if ($1)\n{\n\t$2\n}"
	},
	"if else": {
		"prefix": "ife",
		"body": "if ($1)\n{\n\t$2\n}\nelse\n{\n\t$3\n}"
	},
	"else": {
		"prefix": "else",
		"body": "else\n{\n\t$1\n}"
	},
	"else if": {
		"prefix": "else if",
		"body": "else if ($1)\n{\n\t$2\n}"
	},
	"enum": {
		"prefix": "enum",
		"body": "enum ${MyEnum}\n{\n\t$1\n};"
	},
	"#ifdef": {
		"prefix": "#ifdef",
		"body": "#ifdef ${DEBUG}\n$1\n#endif // ${DEBUG}"
	},
	"#ifndef": {
		"prefix": "#ifndef",
		"body": "#ifndef ${1:1}\n"
	},
	"#if": {
		"prefix": "#if",
		"body": "#ifdef ${1:0}\n$2\n#endif // $1"
	},
	"struct": {
		"prefix": "struct",
		"body": "struct ${MyStruct}\n{\n\t$1\n};"
	},
	"switch": {
		"prefix": "switch",
		"body": "switch (${switch_on})\n{\ndefault:\n\tbreak;$1\n}"
	},
	"union": {
		"prefix": "union",
		"body": "union ${MyUnion}\n{\n\t$1\n};"
	},
	"#inc": {
		"prefix": "#inc",
		"body": "#include \"$0\""
	},
	"#inc<": {
		"prefix": "#inc<",
		"body": "#include <$0>"
	},
	"#def": {
		"prefix": "#def",
		"body": "#define \"$1\" \"$2\""
	}
}
